<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="631.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dsavisualizer.PerformanceAnalysisPage">
    <children>
        <Pane prefHeight="630.0" prefWidth="700.0">
         <children>
            <Pane layoutY="30.0" prefHeight="150.0" prefWidth="400.0">
               <children>
                  <Label layoutX="14.0" layoutY="14.0" text="Linked List">
                     <font>
                        <Font name="Georgia Bold Italic" size="14.0" />
                     </font>
                  </Label>
                  <Button id="listInsertButton" layoutX="14.0" layoutY="62.0" mnemonicParsing="false" text="Insertion (random positions)" />
                  <Button id="listRemoveButton" layoutX="200.0" layoutY="62.0" mnemonicParsing="false" text="Removal (random positions)" />
               </children></Pane>
            <Pane layoutY="180.0" prefHeight="150.0" prefWidth="400.0">
               <children>
                   <Label layoutX="14.0" layoutY="14.0" text="Stack">
                       <font>
                           <Font name="Georgia Bold Italic" size="14.0" />
                       </font>
                   </Label>
                   <ComboBox id="stackImplSpecifier" layoutX="236.0" layoutY="10.0" prefWidth="150.0" promptText="Implementation">
                       <items>
                           <FXCollections fx:factory="observableArrayList">
                               <String fx:value="Array-Based" />
                               <String fx:value="Link-Based" />
                           </FXCollections>
                       </items>
                   </ComboBox>
                  <Button id="stackInsertButton" layoutX="14.0" layoutY="62.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="166.0" text="Insertion" />
                  <Button id="stackRemoveButton" layoutX="200.0" layoutY="62.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="165.0" text="Removal" />
               </children></Pane>
            <Pane layoutY="330.0" prefHeight="150.0" prefWidth="400.0">
               <children>
                   <Label layoutX="14.0" layoutY="14.0" text="Queue">
                       <font>
                           <Font name="Georgia Bold Italic" size="14.0" />
                       </font>
                   </Label>
                   <ComboBox id="queueImplSpecifier" layoutX="236.0" layoutY="10.0" prefWidth="150.0" promptText="Implementation">
                       <items>
                           <FXCollections fx:factory="observableArrayList">
                               <String fx:value="Array-Based" />
                               <String fx:value="Link-Based" />
                           </FXCollections>
                       </items>
                   </ComboBox>
                  <Button id="queueInsertButton" layoutX="14.0" layoutY="62.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="166.0" text="Insertion" />
                  <Button id="queueRemoveButton" layoutX="200.0" layoutY="62.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="165.0" text="Removal" />
               </children></Pane>
            <Pane layoutY="480.0" prefHeight="150.0" prefWidth="400.0">
               <children>
                   <Label layoutX="14.0" layoutY="14.0" text="Heap">
                       <font>
                           <Font name="Georgia Bold Italic" size="14.0" />
                       </font>
                   </Label>
                  <Button id="heapInsertButton" layoutX="14.0" layoutY="62.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="166.0" text="Insertion" />
                  <Button id="heapRemoveButton" layoutX="200.0" layoutY="62.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="165.0" text="Removal" />
               </children></Pane>
            <Pane layoutX="400.0" layoutY="92.0" prefHeight="509.0" prefWidth="296.0">
               <children>
                  <LineChart id="visualizationChart" prefHeight="252.0" prefWidth="288.0">
                    <xAxis>
                      <CategoryAxis side="BOTTOM" />
                    </xAxis>
                    <yAxis>
                      <NumberAxis side="LEFT" />
                    </yAxis>
                  </LineChart>
               </children>
            </Pane>
         </children></Pane>
        <Pane layoutY="630.0" prefHeight="41.0" prefWidth="700.0">
            <children>
                <Button layoutX="652.0" layoutY="8.0" mnemonicParsing="false" onAction="#switchToHomePage" text="Back" />
            </children>
        </Pane>
    </children>
</AnchorPane>
